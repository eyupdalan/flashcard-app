// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Card {
  id        Int     @id @default(autoincrement())
  type      Int
  front     String
  back      String
  known     Boolean @default(false)
  hint      String?
}